    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "pug": "^3.0.3",
    "rollup-plugin-delete": "^3.0.0",
    "sass": "^1.85.1",
