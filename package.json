{
  "name": "@posva/splitpanes",
  "publishConfig": {
    "access": "public"
  },
  "version": "4.0.4",
  "description": "A Vue.js reliable, simple and touch-ready panes splitter / resizer",
  "author": "Antoni Andre <antoniandre.web@gmail.com>",
  "homepage": "https://antoniandre.github.io/splitpanes",
  "repository": "https://github.com/antoniandre/splitpanes",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antoniandre",
  "main": "./dist/splitpanes.js",
  "module": "./dist/splitpanes.js",
  "types": "./dist/splitpanes.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/splitpanes.js",
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "type": "module",
  "keywords": [
    "splitpanes",
    "split panes",
    "panes resizer",
    "vue",
    "vue3",
    "vuejs",
    "ui"
